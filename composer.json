{
    "name": "williambe/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "vcs",
            "url": "git@github.com:williambe/wb_profile.git"
        },
        {
            "type": "git",
            "url": "git@bitbucket.org:williambe/avvguerra-starter.git"
        },
        {
            "type": "git",
            "url": "git@bitbucket.org:williambe/wb_article.git"
        },
        {
            "type": "git",
            "url": "git@bitbucket.org:williambe/wb_core.git"
        },
        {
            "type": "git",
            "url": "git@bitbucket.org:williambe/wb_paragraphs.git"
        },
        {
          "type": "vcs",
          "url": "https://git.drupal.org/project/phingdrushtask.git"
        },
        {
          "type": "package",
          "package": {
            "name": "enyo/dropzone",
            "version": "4.3.0",
            "type": "drupal-library",
            "dist": {
              "url": "https://github.com/enyo/dropzone/archive/v4.3.0.zip",
              "type": "zip"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "kenwheeler/slick",
            "version": "1.6.0",
            "type": "drupal-library",
            "dist": {
              "url": "https://github.com/kenwheeler/slick/archive/master.zip",
              "type": "zip"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "dinbror/blazy",
            "version": "1.8.2",
            "type": "drupal-library",
            "dist": {
              "url": "https://github.com/dinbror/blazy/archive/1.8.2.zip",
              "type": "zip"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "fengyuanchen/cropper",
            "version": "v3.0.0-beta",
            "type": "drupal-library",
            "dist": {
              "url": "https://github.com/fengyuanchen/cropper/archive/master.zip",
              "type": "zip"
            }
          }
        },
        {
            "type": "package",
            "package": {
                "name": "desandro/masonry",
                "version": "3.3.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/desandro/masonry/archive/v3.3.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "desandro/imagesloaded",
                "version": "3.1.8",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/desandro/imagesloaded/archive/v3.1.8.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "drupal-composer/drupal-scaffold": "^2.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/core": "~8.0",
        "drush/drush": "~8.0",
        "drupal/console": "~1.0",
        "williambe/wb_profile": "dev-master",
        "williambe/avvguerra-starter": "dev-avvguerra-starter",
        "drupal/ui_patterns": "^1.0@beta",
        "drupal/field_group": "^1.0@RC",
        "drupal/features": "^3.2",
        "drupal/ds": "dev-2.x",
        "davidbarratt/custom-installer": "^1.0@alpha",
        "williambe/wb_core": "dev-master",
        "williambe/wb_article": "dev-master",
        "williambe/wb_paragraphs": "dev-master",
        "drupal/bg_image_formatter": "^1.0@beta",
        "drupal/simple_background_image_formatter": "^1.1",
        "drupal/config_installer": "^1.4",
        "drupal/twig_field_value": "^1.1",
        "drupal/image_url_formatter": "^1.0@beta",
        "drupal/focal_point": "dev-1.x",
        "drupal/config_update": "^1.3",
        "drupal/media_entity": "^1.6",
        "drupal/media_entity_image": "^1.2",
        "drupal/entity_browser": "^1.0@RC",
        "drupal/dropzonejs": "^1.0@alpha",
        "drupal/blazy": "^1.0@RC",
        "enyo/dropzone": "^4.3",
        "dinbror/blazy": "^1.8"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phing/phing": "~2.10",
        "drupal/phingdrushtask": "^1.1",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "custom-installer": {
            "web/modules/features/{$name}": ["type:drupal-features-module"]
        },
        "_comment_patches": "Temporarily enabling patches on chained composer.json until config_installer is on core",
        "patches": {
            "enable-patching" : true,
            "drupal/core": {
                "Stable theme does not respect visually hidden field label display option": "https://www.drupal.org/files/issues/stable_theme_does_not-2779919-10.patch"     
            }

        }
    }
}
